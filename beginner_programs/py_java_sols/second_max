import java.lang.Math;public class second_max{static int[] list1;static int mx;static int secondmax;static int n;public static void main(String[]args){list1=new int[] {10,20,4,45,99};mx=Math.max(list1[0], list1[1]);secondmax=Math.min(list1[0], list1[1]);n=list1.length;for(int i=2;i<n;i++){if(list1[i]>mx){secondmax=mx;mx=list1[i];}else if(list1[i]>secondmax&&mx!=list1[i]){secondmax=list1[i];}}System.out.printf("Second highest number is :  %s\n",String.valueOf(secondmax));}}