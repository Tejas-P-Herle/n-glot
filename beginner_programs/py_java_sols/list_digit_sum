import java.util.Arrays;import java.util.ArrayList;public class list_digit_sum{static int[] test_list;static ArrayList<Integer> res;static int sum;public static void main(String[]args){test_list=new int[] {12,67,98,34};System.out.println("The original list is : "+Arrays.toString(test_list));res=new ArrayList<Integer>();for(int ele:test_list){sum=0;for(char digit:String.valueOf(ele).toCharArray()){sum=sum+Character.getNumericValue(digit);}res.add(sum);}System.out.println("List Integer Summation : "+res.toString());}}