public class calc{public static void main(String[]args){int a=1;int b=2;String opr="/";calc(a,b,opr);}static void calc(int a,int b,String opr){if(opr.equals("+")){System.out.printf("SUM %d\n",a+b);}else if(opr.equals("-")){System.out.printf("SUB %d\n",a-b);}else if(opr.equals("*")){System.out.printf("MUL %d\n",a*b);}else if(opr.equals("/")){System.out.printf("DIV %f\n",(double)a/b);}}}