import java.util.Arrays;public class elem_in_list{static int[] test_list;public static void main(String[]args){test_list=new int[] {1,6,3,5,3,4};System.out.println("Checking if 4 exists in list ( using loop ) : ");for(int i:test_list){if((i==4)){System.out.println("Element Exists");}}System.out.println("Checking if 4 exists in list ( using in ) : ");if((Arrays.stream(test_list).anyMatch(j -> j == 4))){System.out.println("Element Exists");}}}